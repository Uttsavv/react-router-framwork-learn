        "db:migrate": "drizzle-kit migrate",
        "db:drop": "drizzle-kit drop"
    },
    "dependencies": {
        "@react-router/node": "^7.1.1",
        "@react-router/serve": "^7.1.1",
        "dotenv": "^16.4.7",
        "drizzle-orm": "^0.38.3",
        "drizzle-zod": "^0.6.1",
        "isbot": "^5.1.17",
        "match-sorter": "^8.0.0",
        "pg": "^8.13.1",
        "react": "^19.0.0",
        "react-dom": "^19.0.0",
        "react-router": "7.0.2",
        "sort-by": "^1.2.0",
        "tiny-invariant": "^1.3.3",
        "zod": "^3.24.1"
    },
    "devDependencies": {
        "@react-router/dev": "^7.1.1",
        "@types/node": "^20",
        "@types/pg": "^8.11.10",
        "@types/react": "^19.0.1",
        "@types/react-dom": "^19.0.1",
        "cross-env": "^7.0.3",
        "drizzle-kit": "^0.30.1",
        "tsx": "^4.19.2",
